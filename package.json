{
  "name": "survv",
  "version": "1.0.0",
  "description": "SURVV Web Frontend",
  "author": "Virgin Gates",
  "license": "UNLICENSED",
  "private": true,
  "homepage": "https://github.com/VirginGates/survv-fe-c-web#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VirginGates/survv-fe-c-web.git"
  },
  "keywords": [
    "survv",
    "virgin gates",
    "virgin gates web"
  ],
  "bugs": {
    "url": "https://github.com/VirginGates/survv-fe-c-web/issues"
  },
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "resolutions": {
    "date-fns": "^2.9.0"
  },
  "scripts": {
    "bundle": "node ./scripts/nginx-config-generator && NODE_ENV=production webpack --progress --mode production",
    "dev": "NODE_ENV=development webpack-dev-server --progress --mode development",
    "dups": "yarn workspaces run dups",
    "lint": "eslint --ext .js,.vue,.ts --ignore-path .gitignore ./",
    "lint:staged": "bash ./scripts/lint-staged.sh",
    "type-check": "tsc",
    "test": "yarn workspaces run test",
    "pre-commit": "bash ./scripts/lint-staged.sh",
    "pre-push": "npm run type-check && npm run lint && npm run dups && npm run coverage",
    "coverage": "yarn workspaces run coverage",
    "e2e": "yarn workspaces run e2e",
    "build": "npm run lint && npm run type-check && npm run dups && npm run coverage && npm run e2e"
  },
  "dependencies": {
    "@survv/commons": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/register": "^7.11.5",
    "@types/chai": "^4.2.12",
    "@types/mapbox-gl": "^1.12.1",
    "@types/mocha": "^8.0.3",
    "@types/sinon": "^9.0.5",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "autoprefixer": "^10.0.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-istanbul": "^6.0.0",
    "browserslist": "^4.16.5",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.1.1",
    "css-loader": "^4.3.0",
    "eslint": "^7.9.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-sort-imports-es6-autofix": "^0.5.0",
    "eslint-plugin-vue": "^6.2.1",
    "eslint-plugin-vuetify": "^1.0.0-beta.7",
    "fibers": "^5.0.0",
    "file-loader": "^6.1.0",
    "fork-ts-checker-webpack-plugin": "^5.2.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.3.0",
    "jsinspect": "^0.12.7",
    "mini-css-extract-plugin": "^0.11.2",
    "mocha": "^8.1.3",
    "nyc": "^15.1.0",
    "postcss": "^8.2.10",
    "postcss-loader": "^4.0.2",
    "prettier": "^2.1.2",
    "sass": "~1.32.12",
    "sass-loader": "^10.0.2",
    "style-loader": "^1.2.1",
    "svg-inline-loader": "^0.8.2",
    "ts-loader": "^8.0.4",
    "typescript": "4.3.5",
    "url-loader": "^4.1.0",
    "vue-eslint-parser": "^7.1.0",
    "vue-loader": "^15.9.3",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.12",
    "vuetify-loader": "^1.6.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.1.4",
    "webpack-node-externals": "^2.5.2",
    "workbox-webpack-plugin": "^5.1.4"
  }
}
